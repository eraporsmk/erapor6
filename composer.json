{
  "name": "laravel/laravel",
  "type": "project",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3|^8.0",
    "carlos-meneses/laravel-mpdf": "^2.1",
    "doctrine/dbal": "^2.2",
    "fideloper/proxy": "^4.4",
    "fruitcake/laravel-cors": "^2.0",
    "gecche/laravel-multidomain": "4.0",
    "gonoware/laravel-maps": "^1.4",
    "guzzlehttp/guzzle": "^7.0.1",
    "jantinnerezo/livewire-alert": "^2.2",
    "laravel/framework": "^8.40",
    "laravel/jetstream": "^2.9",
    "laravel/sanctum": "^2.15",
    "laravel/tinker": "^2.5",
    "larswiegers/laravel-maps": "^0.12.0",
    "lavary/laravel-menu": "^1.8",
    "livewire/livewire": "^2.5",
    "maatwebsite/excel": "^3.1",
    "pharaonic/livewire-select2": "^1.1",
    "rap2hpoutre/fast-excel": "^3.2",
    "riskihajar/terbilang": "^1.2",
    "santigarcor/laratrust": "^7.1",
    "staudenmeir/eloquent-eager-limit": "^1.6",
    "staudenmeir/eloquent-has-many-deep": "^1.14"
  },
  "require-dev": {
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.2",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.3.3",
    "pixinvent/jetstrap-vuexy": "dev-main"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/Helpers/helpers.php",
	  "app/Helpers/erapor.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi",
	  "@php artisan vendor:publish --force --tag=livewire-assets"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  }
}
